{
    "family": "kotech-hub-manager",
    "containerDefinitions": [
        {
            "name": "hub-manager",
            "image": "864172178598.dkr.ecr.ap-northeast-2.amazonaws.com/kotech-hub-manager-repository:%GIT_HASH%",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/kotech-hub-manager",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::864172178598:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}